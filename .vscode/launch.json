{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000/activities",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "skipFiles": ["node_modules/**", "<node_internals>/**"]
    },
    {
      "name": "Next.js: Node.js",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    }
  ]
}
